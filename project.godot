[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/phantom_camera/plugin.cfg")

[filesystem]

import/fbx2gltf/enabled=false
import/blender/enabled=false

